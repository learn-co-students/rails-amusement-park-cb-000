<h1>Register Here</h1>
<br>
<%= form_for :user, url: '/users' do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.label :height %>
  <%= f.text_field :height %>
  <br>
  <%= f.label :happiness %>
  <%= f.text_field :happiness %>
  <br>
  <%= f.label :nausea %>
  <%= f.text_field :nausea %>
  <br>
  <%= f.label :tickets %>
  <%= f.text_field :tickets %>
  <br>
  <%= f.label :admin %>
  <%= f.check_box :admin %>
  <br>
  <%= f.submit "Create User" %>
<% end %>
